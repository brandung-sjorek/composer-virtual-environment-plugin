{
	"keywords" : [
		"environment",
		"shell",
		"php",
		"composer"
	],
	"autoload" : {
		"psr-4" : {
			"Sjorek\\Composer\\" : "src"
		},
		"psr-0" : {
			"Sjorek\\Composer\\" : "src"
		}
	},
	"name" : "sjorek/composer-virtual-environment-plugin",
	"type" : "composer-plugin",
	"description" : "Provides a composer-plugin that adds a command to activate/deactivate the current bin-directory in shell, optionally creating symlinks to the composer- and php-binary in the bin-directory.",
	"license" : "BSD-3-Clause",
	"require" : {
		"composer-plugin-api" : "^1.1"
	},
	"extra" : {
		"class" : "Sjorek\\Composer\\VirtualEnvironmentPlugin"
	},
	"homepage" : "http://github.com/sjorek/composer-virtual-environment-plugin",
	"authors" : [{
			"name" : "Stephan Jorek",
			"email" : "stephan.jorek@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.8@dev"
	}
}
